{
  "app-id": "de.wwwtech.ColorMate",
  "base": "io.elementary.BaseApp",
  "base-version": "juno-23.08",
  "runtime": "org.freedesktop.Platform",
  "runtime-version": "23.08",
  "sdk": "org.freedesktop.Sdk",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "command": "colormate",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--socket=wayland"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin",
    "env": {
      "CARGO_HOME": "cargo",
      "RUST_BACKTRACE": "1",
      "RUST_LOG": "colormate=debug"
    }
  },
  "modules": [
    {
      "name": "colormate",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/ckruse/ColorMate/archive/refs/tags/1.0.0.tar.gz",
          "sha256": "0cae31a2dc817cea2a63042ae21b33c3c1059b4c97514497a28c0eab55222921"
        }
      ]
    }
  ]
}
